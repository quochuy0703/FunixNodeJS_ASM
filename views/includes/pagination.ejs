<nav aria-label="Page navigation example">
  <ul class="pagination justify-content-center">
    <li class="page-item <%= currentPage === 1? 'disabled':''%>">
      <a class="page-link" href="/work-hour/?page=<%= currentPage-1%>"
        >Previous</a
      >
    </li>
    <% if (currentPage !== 1 && previousPage !== 1) { %>
    <li class="page-item <%= currentPage=== 1 ? 'active':''%>">
      <a class="page-link" href="/work-hour/?page=1">1</a>
    </li>
    <% } %> <% if (hasPreviousPage) { %>
    <li class="page-item">
      <a class="page-link" href="/work-hour/?page=<%= previousPage%>"
        ><%= previousPage%></a
      >
    </li>
    <% } %>
    <li class="page-item active">
      <a class="page-link" href="/work-hour/?page=<%= currentPage%>"
        ><%= currentPage%></a
      >
    </li>
    <% if (hasNextPage) { %>

    <li class="page-item">
      <a class="page-link" href="/work-hour/?page=<%= nextPage%>"
        ><%= nextPage%></a
      >
    </li>
    <% } %> <% if (lastPage !== currentPage && nextPage !== lastPage) { %>

    <li class="page-item">
      <a class="page-link" href="/work-hour/?page=<%= lastPage%>"
        ><%= lastPage%></a
      >
    </li>
    <% } %>
    <li class="page-item <%= currentPage === lastPage? 'disabled':''%>">
      <a class="page-link" href="/work-hour/?page=<%= currentPage+1%>">Next</a>
    </li>
  </ul>
</nav>
